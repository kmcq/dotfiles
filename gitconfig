[push]
  default = current
[color]
  ui = auto
[alias]
  ci = commit
  co = checkout
  cp = cherry-pick
  delete-merged-branches = "!git co master && git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
  pruneremote = remote prune origin
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[fetch]
  prune = true
[rebase]
  autosquash = true
[user]
  name = "Keith McHugh"
  email = "keith.mchugh@gmail.com"
